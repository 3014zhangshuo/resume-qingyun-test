<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">


<% @orders.each do |o| %>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>订单编号</td>
        <td>简历编号</td>
        <td>发送购买码</td>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= o.id %></td>
        <td><%= o.resume_id %></td>
        <td><%= link_to("发送购买码", send_code_admin_order_path(o), method: :post, class: "btn btn-primary") %></td>
      </tr>
    </tbody>
  </table>
<% end %>
</div>

</div>
</div>
