
<div class="container" style="margin-bottom:300px;">
<div class="row">
  <% @user.resumes.each do |resume| %>
    <% if !resume.resume_html.present? %>
     <div class="col-md-3 col-sm-6 col-xs-12">
         <div class="product-item">
           <div class="pi-img-wrapper">
             <!-- <img src="" class="img-responsive" alt="ResumeHere"> -->
             <%= image_tag "sampleresume.jpg", class: "img-responsive" %>

           </div>
           <h3>
             <% if resume.resume_name.present? %>
              <%= resume.resume_name %>
             <% else %>
              [未命名简历]
             <% end %>
            <span class="pull-right">&nbsp;未生成</span><i class="fa fa-file-text-o pull-right status-r"></i>
           </h3>
           <div class="pi-price">创建于：<%= resume.created_at.strftime("%m/%e, %l:%M %p") %>
            &nbsp;&nbsp;&nbsp; <%= link_to("删除", admin_user_resume_path(@user,resume), method: :delete, class: "btn btn-xs btn-danger", data: { confirm: "确定删除这个未完成的简历？" }) %></div>
         </div>
      </div>
     <% end %>
    <% end %>
</div>
