
	<div class="modal-dialog" style="margin-top:10%;">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
				<h4 class="modal-title" id="myModalLabel">测试版即将上线，敬请期待！</h4>
			</div>
			<div class="modal-body">
			  <div class="row">
				  <div class="col-md-6 col-xs-12">
					  <div class="well">
						<%= simple_form_for Tester.new do |f| %>
							<%= f.error_notification %>
							<div class="form-inputs">
								<%= f.input :name, label: "姓名", required: true, autofocus: true %>
								<%= f.input :occupation, label: "您想申请的工作所属行业", required: true %>
								<%= f.input :email, label: "邮箱", required: true %>
							</div>
						</div>
					</div>
			<div class="col-md-6 col-xs-12" style="text-align:center">
				 <br><p class="lead">率先使用简历黑客，招聘旺季领先他人</p>
				 <ul class="list-unstyled" style="line-height: 2.5; text-align:left; margin-left:10%">
				 </ul>
			 </div>
				 <div style="text-align: center;">
					 <br>
					 <%= f.button :submit, "提交", class: "btn btn-primary btn-lg" %>
				 </div>
		 </div>
	 </div>
			<div class="modal-footer">
			<div class="form-actions">

			</div>
			<% end %>
			</div>
		</div>
	</div>
<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
